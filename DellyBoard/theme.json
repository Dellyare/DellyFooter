{
    "name": "DellyBoard",
    "author": "Dellyare",
    "manifest_version": 4,
    "version": "v1.0",
    "target": "Keyboard",
    "description": "Keyboards in Apple style",
    "inject": {
        "shared.css": ["SP"]
    },
    "patches": {
        "Dark theme": {
            "default": "Yes",
            "type": "checkbox",
            "values": {
                "No": {
                    "light.css": ["SP"]
                },
                "Yes": {
                    "dark.css": ["SP"]
                }
            }
        }
    }
}